{
  "name": "server",
  "version": "0.0.1",
  "scripts": {
    "prestart": "tsc --build",
    "start-build": "tsc --build --watch",
    "start-server": "nodemon dist/main.js",
    "start": "concurrently --kill-others \"yarn start-build\" \"yarn start-server\""
  },
  "devDependencies": {
    "@types/express": "^4.17.12",
    "concurrently": "^6.2.0",
    "nodemon": "^2.0.9",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "express": "^4.17.1"
  }
}
